{
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "1.0.1",
    "permissions": [
      "storage"
    ],
    "default_locale": "en",
    "browser_action": {
      "default_popup": "src/popup.html",
      "default_icon": {
        "16": "icons/note16v2.png",
        "48": "icons/note48v2.png",
        "128": "icons/note128v2.png"
      }
    },
    "icons": {
      "16": "icons/note16v2.png",
      "48": "icons/note48v2.png",
      "128": "icons/note128v2.png"
    },
    "background": {
      "scripts": ["dist/js/background.js"],
      "persistent": false
    }
  }  
