{
    "manifest_version": 3,
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "version": "1.0",
    "permissions": [
        "storage"
    ],
    "default_locale": "en",
    "action": {
        "default_popup": "src/popup.html",
        "default_icon": {
            "16": "icons/note16.png",
            "48": "icons/note48.png",
            "128": "icons/note128.png"
        }
    },
    "icons": {
        "16": "icons/note16.png",
        "48": "icons/note48.png",
        "128": "icons/note128.png"
    },
    "background": {
        "service_worker": "dist/background.js"
    }
}
